import java.net.Socket;

public class HttpRequest implements Runnable {
	final String CRLF = "\r\n";
	Socket socket;
	public HttpRequest(Socket socket) throws Exception
	{
		this.socket = socket;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		try
		{
			processRequest();
		}
		catch (Exception e)
		{
			System.out.println(e);
		}
	}
	
